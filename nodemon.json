{
    "restartable": "rs",
    "ignore": [".git", "node_modules/", "dist/", "coverage/"],
    "watch": [
        "src",
        "server.js"
    ],
    "execMap": {
      "ts": "node -r ts-node/register"
    },
    "env": {
      "NODE_ENV": "development",
      "NODE_OPTIONS": "--trace-warnings"
    },
    "ext": "js,json,ts"
}